{  
   "src_folders":"./tests/e2e/groups/",
   "output_folder":"./reports/e2e/",
   "globals_path":"./tests/e2e/globals.js",
   "page_objects_path":"./tests/e2e/pages/",
   "custom_commands_path":"./tests/e2e/custom-commands",
   "selenium":{  
      "start_process":true,
      "server_path":"./node_modules/selenium-standalone/.selenium/selenium-server/2.50.1-server.jar",
      "log_path":"",
      "host":"127.0.0.1",
      "port":4444,
      "cli_args":{  
         "webdriver.chrome.driver":"./node_modules/selenium-standalone/.selenium/chromedriver/2.21-x64-chromedriver"
      }
   },
   "test_settings":{  
      "chrome":{  
         "skip_testcases_on_fail":false,
         "silent":true,
         "screenshots":{  
            "enabled":false,
            "on_failure":false,
            "on_error":false,
            "path":"./tests/screenshots"
         },
         "desiredCapabilities":{  
            "browserName":"chrome"
         }
      },
      "phantom":{  
         "skip_testcases_on_fail":false,
         "screenshots":{  
            "enabled":false,
            "on_failure":false,
            "on_error":false,
            "path":"./tests/screenshots"
         },
         "desiredCapabilities":{  
            "browserName":"phantomjs",
            "phantomjs.binary.path":"./node_modules/phantomjs/bin/phantomjs"
         }
      }
   },
   "nexus-5":{  
      "silent":true,
      "skip_testcases_on_fail":false,
      "screenshots":{  
         "enabled":false,
         "on_failure":false,
         "on_error":false,
         "path":"./tests/screenshots"
      },
      "desiredCapabilities":{  
         "browserName":"chrome",
         "chromeOptions":{  
            "mobileEmulation":{  
               "deviceName":"Google Nexus 5"
            },
            "args":[  
               "incognito",
               "disable-extensions"
            ]
         }
      }
   },
   "nexus-7":{  
      "silent":true,
      "skip_testcases_on_fail":false,
      "screenshots":{  
         "enabled":false,
         "on_failure":false,
         "on_error":false,
         "path":"./tests/screenshots"
      },
      "desiredCapabilities":{  
         "browserName":"chrome",
         "chromeOptions":{  
            "mobileEmulation":{  
               "deviceName":"Google Nexus 7"
            },
            "args":[  
               "incognito",
               "disable-extensions"
            ]
         }
      }
   },
   "chrome-remote":{  
      "skip_testcases_on_fail":false,
      "selenium_host":"ondemand.saucelabs.com",
      "selenium_port":80,
      "username":"${SAUCE_USERNAME}",
      "access_key":"${SAUCE_ACCESS_KEY}",
      "use_ssl":false,
      "silent":true,
      "output":true,
      "screenshots":{  
         "enabled":false,
         "path":"./tests/screenshots"
      },
      "desiredCapabilities":{  
         "browserName":"chrome",
         "chromeOptions":{  
            "mobileEmulation":{  
               "deviceName":"Google Nexus 5"
            },
            "args":[  
               "incognito",
               "disable-extensions"
            ]
         },
         "name":"[${TRAVIS_REPO_SLUG}]:(${TRAVIS_BRANCH})-${TRAVIS_BUILD_NUMBER}",
         "tunnelIdentifier":"${TRAVIS_JOB_NUMBER}"
      },
      "selenium":{  
         "start_process":false
      }
   }
}